<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="wii.edu.core.model.Ruang" table="RUANG">
    <id column="RUANG_ID" name="id">
      <generator class="native"/>
    </id>
    <property column="kode" length="10" name="kode" type="string"/>
    <property column="NAMA" length="150" name="nama" type="string"/>
    <property column="KAPASITAS" length="3" name="kapasitas" type="integer"/>
  </class>
</hibernate-mapping>
