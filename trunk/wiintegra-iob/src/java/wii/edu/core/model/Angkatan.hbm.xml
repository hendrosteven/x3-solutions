<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="wii.edu.core.model.Angkatan" table="ANGKATAN">
    <id column="ANGKATAN_ID" name="id">
      <generator class="native"/>
    </id>
    <property column="NAMA" length="100" name="nama" type="string"/>
    <property column="KETERANGAN" length="200" name="keterangan" type="string"/>
    <property column="BIAYASKS" name="biayaPerSks" type="double"/>
  </class>
</hibernate-mapping>
