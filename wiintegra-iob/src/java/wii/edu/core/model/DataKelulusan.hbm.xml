<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="wii.edu.core.model.DataKelulusan" table="DATA_KELULUSAN">
    <id column="DATA_KELULUSAN_ID" name="id">
      <generator class="native"/>
    </id>
    <property column="TGL_UJIAN" name="tglUjian" type="date"/>
    <property column="TGL_YUDISIUM" name="tglYudisium" type="date"/>
    <property column="JUDUL_SKRIPSI" length="200" name="judulSkripsi" type="string"/>
    <property column="NILAI" length="1" name="nilai" type="string"/>
    <property column="PREDIKAT" length="50" name="predikat" type="string"/>
  </class>
</hibernate-mapping>
