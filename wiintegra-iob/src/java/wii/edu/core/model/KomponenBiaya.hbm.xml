<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="wii.edu.core.model.KomponenBiaya" table="KOMPONEN_BIAYA">
    <id column="KONPONEN_ID" name="id">
      <generator class="native"/>
    </id>
    <property column="KODE" length="10" name="kode" type="string"/>
    <property column="NAMA" length="200" name="nama" type="string"/>
    <property column="KETERANGAN" length="200" name="keterangan" type="string"/>
    <property column="JUMLAH" name="jumlah" type="double"/>
    <property column="IS_SKS" length="1" name="isSKS" type="integer"/>
  </class>
</hibernate-mapping>
